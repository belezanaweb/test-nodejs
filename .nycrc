{
  "temp-directory": "./coverage/.nyc_output",
  "include": [
    "server/**/*.js"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/test/**",
    "**/coverage/**"
  ],
  "check-coverage": true,
  "all": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "reporter": [
    "text",
    "text-summary",
    "lcov"
  ],
  "sourceMap": false
}
