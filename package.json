{
  "name": "test-nodejs",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --no-cache",
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --inspect src/index.ts",
    "prod": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belezanaweb/test-nodejs.git"
  },
  "keywords": [],
  "author": "Beleza na Web <sysadmin@belezanaweb.com.br> (https://www.belezanaweb.com.br)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/belezanaweb/test-nodejs/issues"
  },
  "homepage": "https://github.com/belezanaweb/test-nodejs#readme",
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-serve-static-core": "^4.16.2",
    "@types/joi": "^14.3.2",
    "@types/lokijs": "^1.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-joi-validator": "^2.0.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.6",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "./spec"
    ]
  }
}
