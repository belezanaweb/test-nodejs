{
   "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "dist",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": true
   },
   "exclude": ["node_modules", "dist", "typings"],
   "include": [
      "src/"
, "test"    ]
}
