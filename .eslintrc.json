{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint/eslint-plugin"
    ],
    "rules": {
        "no-useless-constructor": 0,
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "indent": ["error", 4],
        "no-multi-spaces": ["error"]
    }
}
